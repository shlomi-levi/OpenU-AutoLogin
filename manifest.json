{
    "manifest_version": 3,
    "name": "OpenU Auto Login",
    "version": "1.0.1",

    "description": "Automatically logs in to openu",
    "author": "Shlomi Levi",
    "options_page": "./options.html",
    "permissions": ["storage"],

    "content_scripts": [
        {
            "matches": [
                "https://sso.apps.openu.ac.il/login?T_PLACE=https://sheilta.apps.openu.ac.il/pls/dmyopt2/sheilta.main"
            ],
            "js": ["./dst/script.js"],
            "runAt": "document_end"
        }
    ]
}
